        android:shadowDx="1.5"
        android:shadowDy="1.3"
        android:shadowRadius="10"
        android:textColor="@color/black"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/txtNameShow"
        app:layout_constraintEnd_toEndOf="@+id/txtClassShow"
        app:layout_constraintStart_toStartOf="@+id/txtRacaShow"
        app:layout_constraintTop_toBottomOf="@+id/txtClassShow" />

    <TextView
